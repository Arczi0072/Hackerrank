#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int t;
    cin >> t;
    for(int i=0;i<t;i++)
    {
        int a,b;
        cin >> a >> b;
        float c;
        int ctr=0;
        for(int j=a;j<=b;j++)
        {   
            c=sqrt(j);
            if((c==(int)c))
                {
                ctr++;
            }

        }
                 cout << ctr << endl;
        
        
    }
    return 0;
}

